	function calculDistance($x11,$y11,$x22,$y22){
		$pi = 3.14;
		$r = 6378000;
		$d;    
		$x1 = $x11*$pi/180;
		$y1 = $y11*$pi/180;
		$x2 = $x22*$pi/180;
		$y2 = $y22*$pi/180;
		
		$t1 = sin($x1) * sin($x2); 
		$t2 = cos($x1) * cos($x2);
		$t3 = cos($y1 - $y2);

		$d = $r * acos($t1+ $t3 * $t2 );
			
		return round($d/1000, 1);
	}
